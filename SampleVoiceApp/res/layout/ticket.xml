<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent" android:layout_height="fill_parent">

<EditText
    android:id="@+id/text_TicketResult"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:gravity="top|left"
    android:visibility="gone"
    android:minLines="2"  />



<LinearLayout android:layout_height="wrap_content" 
    android:layout_toRightOf="@+id/btn_startTicket" 
    android:id="@+id/layout_bottomButtons" 
    android:layout_alignParentBottom="true" 
    android:layout_width="fill_parent" 
    android:orientation="horizontal">

    <Button
        android:id="@+id/btn_startTicket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="@string/button_startTicket" />

<Button
    android:id="@+id/btn_startTts4"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentRight="true"
    android:layout_weight="1"
    android:text="@string/button_startTts"
    android:visibility="gone" >
</Button>

</LinearLayout>

<LinearLayout
    android:id="@+id/layout_resultsList4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_above="@+id/layout_bottomButtons"
    android:orientation="vertical" >

    <ListView
        android:id="@+id/list_results4"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:paddingLeft="20dip"
        android:paddingRight="20dip"
        android:visibility="gone" >
    </ListView>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="30dip"
        android:minWidth="100dip"
        android:layout_gravity="center"
        android:text="" />
    
    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="30dip"
        android:minWidth="100dip"
        android:text="" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="30dip"
        android:minWidth="100dip"
        android:text="" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="30dip"
        android:minWidth="100dip"
        android:text="" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="30dip"
        android:minWidth="100dip"
        android:text="" />
     

</LinearLayout>

</RelativeLayout>
